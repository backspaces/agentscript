{
    "source": {
        "include": ["src", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs/",
        "recurse": true,

        "verbose": true,
        "template": "node_modules/clean-jsdoc-theme",
        "displayModuleHeader": true,

        "resizeable": {
            "navbar": {
                "min": "300",
                "max": "600"
            }
        },

        "theme_opts": {
            "sections": ["Classes", "Modules", "Global"],
            "default_theme": "dark",
            "sort": false,
            "footer": "AgentScript version: 0.10.19 using clean-jsdoc-theme",
            "menu": [
                {
                    "title": "Github",
                    "id": "github",
                    "link": "https://github.com/backspaces/agentscript"
                },
                {
                    "title": "npm",
                    "id": "npm",
                    "link": "https://www.npmjs.com/package/agentscript"
                }
            ]
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}
